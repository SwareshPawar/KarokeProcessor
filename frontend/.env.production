# Production environment variables
# Platform detection is now automatic based on hostname and environment variables
CI=false

# Vercel will automatically set REACT_APP_VERCEL=1 in their environment
# Render needs REACT_APP_RENDER=1 set manually in their dashboard
# Local development will use neither

REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_NAME=Karaoke Processor

# API URL is now auto-detected, but can be overridden if needed
# REACT_APP_API_URL=/api