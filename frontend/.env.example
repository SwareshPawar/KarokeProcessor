# Frontend Environment Configuration

# API Base URL - change this to your backend server URL
REACT_APP_API_URL=http://localhost:3001/api

# Google OAuth Configuration (must match backend settings)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here

# App Configuration
REACT_APP_NAME=Karaoke Processor
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_GOOGLE_DRIVE=true
REACT_APP_ENABLE_YOUTUBE=true
REACT_APP_ENABLE_KEY_DETECTION=true

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=52428800
REACT_APP_SUPPORTED_FORMATS=mp3,wav,aac,m4a,ogg,flac